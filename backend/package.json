{
  "private": true,
  "name": "backend",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npx tsoa spec-and-routes && tsc && npm run build-docs",
    "check": "tsc --noEmit",
    "start": "node dist/index.js",
    "format": "prettier --write {src,tests}/**/**/*.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage tests/unit tests/integration",
    "test-e2e": "jest tests/e2e --detectOpenHandles",
    "cronjob": "nodemon --exec \"ts-node ./src/bin/air-quality-cron-job.ts -lat 48.856613 -lon 2.352222\"",
    "dev": "npx prisma migrate deploy && concurrently \"nodemon -x tsoa spec-and-routes\" \"nodemon\"",
    "build-docs": "npx @redocly/cli build-docs ./src/generated/openapi.json -o ./src/generated/openapi.html",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "5.7.1",
    "apicache": "^1.6.3",
    "commander": "^11.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^6.6.0",
    "mollitia": "^0.1.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "pino": "^8.17.2",
    "pino-http": "^8.6.1",
    "pino-pretty": "^9.1.0",
    "reflect-metadata": "^0.2.1",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.11.9",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prisma": "5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
